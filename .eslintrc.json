{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 8
  },
  "env": {
    "es2017": true,
    "browser": true,
    "node": true,
    "serviceworker": true,
    "mocha": true
  },
  "settings": {
    "react": {
      "version": "16"
    }
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "import/no-unresolved": "off",
    "no-use-before-define": ["error", {"functions": false}]
  }
}
